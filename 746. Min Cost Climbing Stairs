class Solution:
    def minCostClimbingStairs(self, cost: List[int]) -> int:
        dp = [0,0]
        if len(cost)== 2:
            return (min(cost))
        else:
            for i in range(2,len(cost)+1):
                x = min(dp[i-1]+cost[i-1],dp[i-2]+cost[i-2])
                dp.append(x)
            return dp[-1]


class Solution:
    def minCostClimbingStairs(self, cost: List[int]) -> int:
        dp = [0,0]
        if len(cost)== 2:
            return (min(cost))
        else:
            for i in range(2,len(cost)+1):
                x = min(dp[1]+cost[i-1],dp[0]+cost[i-2])
                dp.append(x)
                dp.pop(0)

            return dp[-1]
