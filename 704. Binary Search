class Solution:
    def search(self, nums: List[int], target: int) -> int:
        lo = 0
        hi = len(nums) - 1
        mid = (lo + hi) // 2
        check = False
   
        if target == nums[mid]:
            return mid 
        else:
            while(lo<=hi):
                if nums[mid] == target:
                    check = True
                    return mid
                if nums[mid] < target:
                    lo = mid + 1
                    mid = (lo + hi) // 2                        
                else:
                    hi = mid - 1
                    mid = (lo + hi) // 2

        if check == False:
            return -1
        
